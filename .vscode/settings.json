// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git"           : true,
        "**/.svn"           : true,
        "**/.hg"            : true,
        "**/.DS_Store"      : true,
        "**/.vs"            : true,
        "Kpp/**/output"  : true,
        "Kpp/Athena/external/asmjit*/tools"  : true,
        "Kpp/Athena/external/asmjit*/*.*"    : true,
        "Kpp/Athena/external/fmt*/support"   : true,
        "Kpp/Athena/external/fmt*/*.*"       : true,
        "Kpp/Athena/external/spdlog*/bench"  : true,
        "Kpp/Athena/external/spdlog*/cmake"  : true,
        "Kpp/Athena/external/spdlog*/example": true,
        "Kpp/Athena/external/spdlog*/tests"  : true,
        "Kpp/Athena/external/spdlog*/*.*"    : true,
        "Kpp/Athena/external/termcolor*/docs": true,
        "Kpp/Athena/external/termcolor*/test": true,
        "Kpp/Athena/external/termcolor*/*.*" : true
    },
    "files.associations": {
        "chrono": "cpp",
        "*hfunctional": "cpp",
        "tuple": "cpp",
        "*h*hxutility": "cpp",
        "*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*h*hregex": "cpp",
        "regex": "cpp",
        "*h*h*h*h*h*h*h*h*h*h*h*h*h*h*CPPfunctional": "cpp",
        "functional": "cpp",
        "*h*h*h*h*hxfunctional": "cpp",
        "xfunctional": "cpp",
        "xlocmon": "cpp",
        "array": "cpp",
        "vector": "cpp",
        "xstring": "cpp",
        "list": "cpp"
    }
}